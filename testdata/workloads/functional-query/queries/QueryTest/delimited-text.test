---- QUERY
# insert data into dhp table and check results
insert into delim_text_test_db.dhp values
('abc $ abc', 'xyz # xyz', 1, 2),
('abc $$$ abc', 'xyz ### xyz', 3, 4),
('abc #$#$ abc', 'xyz $#$# xyz', 5, 6)
---- RESULTS
: 3
